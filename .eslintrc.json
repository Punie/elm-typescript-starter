{
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "@typescript-eslint",
    "jest"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "rules": {
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off",
    "object-curly-newline": "off",
    "@typescript-eslint/array-type": [
      2,
      {
        "default": "generic",
        "readonly": "generic"
      }
    ]
  }
}
