{
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-parens": [
      2,
      "as-needed",
      {
        "requireForBlockBody": true
      }
    ],
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off",
    "object-curly-newline": "off",
    "@typescript-eslint/array-type": [
      2,
      {
        "default": "generic",
        "readonly": "generic"
      }
    ]
  }
}
